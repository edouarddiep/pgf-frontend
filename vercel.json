{
  "version": 2,
  "public": false,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/pgf-frontend/browser",
  "framework": null,
  "alias": ["gonsethfavre.vercel.app"],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
